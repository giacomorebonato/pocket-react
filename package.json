{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:types": "pocketbase-typegen --db ./pb_data/data.db --url http://localhost:8090 --out frontend/pocketbase-types.ts",
    "dev:client": "vite dev",
    "dev:server": "go run . serve",
    "build": "run-s build:*",
    "build:client": "tsc -b && vite build",
    "build:server": "go build -tags production -o pocket-react",
    "format": "pnpm biome check --write ./frontend/",
    "migrate": "pocketbase migrate collections",
    "migrate:sync": "go run ./main.go ./embed_dev.go migrate history-sync"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@monaco-editor/react": "^4.6.0",
    "@tanstack/react-query": "^5.59.16",
    "@tanstack/react-router": "^1.76.1",
    "@tanstack/router-devtools": "^1.76.1",
    "@tanstack/router-plugin": "^1.76.0",
    "pocketbase": "^0.21.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tanstack/react-query-devtools": "^5.0.0",
    "@types/node": "^22.7.9",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.13",
    "monaco-editor": "^0.52.0",
    "npm-run-all": "^4.1.5",
    "pocketbase-typegen": "^1.2.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "vite": "^5.4.9",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.3.0",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
